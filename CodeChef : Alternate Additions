#include<iostream>
using namespace std;
///Problem link : https://www.codechef.com/JUNE221D/problems/ALTERADD
int main(){
    int T;
    cin>>T;
    while(T--){
        int a,b;
        cin>>a>>b;
        int dif = b-a;
        if(dif%3==0||dif%3==1){
            cout<<"YES"<<endl;
        }else cout<<"NO"<<endl;
    }return 0;
}
