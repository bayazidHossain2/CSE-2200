#include<iostream>
using namespace std;
///Problem Link : https://www.codechef.com/submit-v2/DNASTORAGE
int main(){
    int T;
    cin>>T;
    while(T--){
        int n;
        cin>>n;
        char ch1,ch2;
        n/=2;
        while(n--){
            cin>>ch1>>ch2;
            if(ch1=='0'&&ch2=='0'){
                cout<<'A';
            }else if(ch1=='0'&&ch2=='1'){
                cout<<'T';
            }else if(ch1=='1'&&ch2=='0'){
                cout<<'C';
            }else{
                cout<<'G';
            }
        }cout<<endl;
    }return 0;
}
